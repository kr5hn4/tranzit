{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tranzit",
  "version": "0.1.0",
  "identifier": "com.tranzit.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "tranzit",
        "width": 800,
        "height": 600,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "capabilities": ["default"],
      "csp": null
    }
  }
}
